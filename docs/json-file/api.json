  "components": {
    "schemas": {
      "TransformRule": {
        "type": "object",
        "properties": {
          "minZoom": {
            "type": "number",
            "description": "缩放级别范围",
            "minimum": 0,
            "maximum": 25
          },
          "maxZoom": {
            "type": "number",
            "description": "缩放级别范围",
            "minimum": 0,
            "maximum": 25
          },
          "scale": {
            "type": "number",
            "description": "图标缩放比例"
          }
        },
        "required": [
          "scale"
        ]
      },
      "SymbolRule": {
        "type": "object",
        "properties": {
          "filter": {
            "type": "string",
            "description": "按规则展示icon(支持正则),更多filter规则: https://github.com/mapnik/mapnik/wiki/Filter"
          },
          "icon": {
            "type": "string",
            "description": "icon的url(相对路径)"
          }
        },
        "required": [
          "filter",
          "icon"
        ]
      },
      "Symbols": {
        "type": "object",
        "properties": {
          "defaultIcon": {
            "type": "string",
            "description": "默认icon的url(相对路径)",
            "example": "/path/to/file"
          },
          "transform": {
            "description": "根据地图缩放级别缩放图标的大小",
            "example": [
              {
                "maxZoom": 10,
                "scale": 0.1
              },
              {
                "minZoom": 11,
                "maxZoom": 14,
                "scale": 0.5
              },
              {
                "minZoom": 15,
                "maxZoom": 18,
                "scale": 2
              },
              {
                "minZoom": 19,
                "scale": 5
              }
            ],
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/TransformRule"
            }
          },
          "rules": {
            "description": "按规则展示icon(支持正则)",
            "example": [
              {
                "filter": "[status].match('1')",
                "icon": "/path/to/file"
              },
              {
                "filter": "[status] &gt;= '2'",
                "icon": "/path/to/file"
              },
              {
                "filter": "[status] = '3'",
                "icon": "/path/to/file"
              }
            ],
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/SymbolRule"
            }
          }
        },
        "required": [
          "defaultIcon",
          "transform",
          "rules"
        ]
      },
      "DataSourceConnection": {
        "type": "object",
        "properties": {
          "host": {
            "type": "string"
          },
          "port": {
            "type": "number"
          },
          "username": {
            "type": "string"
          },
          "password": {
            "type": "string"
          },
          "database": {
            "type": "string",
            "description": "数据库名称"
          }
        },
        "required": [
          "host",
          "port",
          "username",
          "password",
          "database"
        ]
      },
      "ExtraField": {
        "type": "object",
        "properties": {
          "field": {
            "type": "string"
          },
          "type": {
            "type": "string"
          }
        },
        "required": [
          "field",
          "type"
        ]
      },
      "CreateFrom": {
        "type": "object",
        "properties": {
          "connection": {
            "description": "第三方数据库连接相关配置",
            "example": {
              "host": "localhost",
              "port": 3306,
              "username": "root",
              "password": "root",
              "database": "test"
            },
            "allOf": [
              {
                "$ref": "#/components/schemas/DataSourceConnection"
              }
            ]
          },
          "driver": {
            "type": "string",
            "description": "数据库驱动类型",
            "example": "mysql",
            "enum": [
              "mysql",
              "pg"
            ]
          },
          "tableOrSubQuery": {
            "type": "string",
            "description": "数据库表名或子查询",
            "example": "(select id, pre_design_lon, pre_design_lat, status from m_shot_point_book) as data"
          },
          "uniqueIdField": {
            "type": "string",
            "description": "数据库id的字段名",
            "example": "id"
          },
          "lngField": {
            "type": "string",
            "description": "数据库经度字段名称",
            "example": "longitude"
          },
          "latField": {
            "type": "string",
            "description": "数据库纬度字段名称",
            "example": "latitude"
          },
          "extraFields": {
            "description": "其他字段",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ExtraField"
            }
          }
        },
        "required": [
          "connection",
          "driver",
          "tableOrSubQuery",
          "uniqueIdField",
          "lngField",
          "latField"
        ]
      },
      "CustomDataSource": {
        "type": "object",
        "properties": {
          "connection": {
            "description": "自定义数据源连接配置（只支持postgis）",
            "allOf": [
              {
                "$ref": "#/components/schemas/DataSourceConnection"
              }
            ]
          },
          "tableName": {
            "type": "string",
            "description": "创建图层所需的postgis数据表名称,不填时服务器会自动生成一个postgis数据表"
          },
          "lngField": {
            "type": "string",
            "description": "数据库经度字段名称（根据此字段生成空间数据列）",
            "example": "longitude"
          },
          "latField": {
            "type": "string",
            "description": "数据库纬度字段名称 （根据此字段生成空间数据列）",
            "example": "latitude"
          }
        },
        "required": [
          "tableName"
        ]
      },
